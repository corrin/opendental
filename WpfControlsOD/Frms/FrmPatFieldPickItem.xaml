<local:FrmODBase x:Class="OpenDental.FrmPatFieldPickItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="323" Height="138" 
             Text="Patient Field Pick Item" MinMaxBoxes="False">
	<Grid Background="#FFFCFDFE" >
		<ui:Button x:Name="butDelete" Width="84" VerticalAlignment="Bottom" Height="24" Margin="10,0,0,9" Text="_Delete" Click="butDelete_Click" Icon="DeleteX" />
		<ui:Button x:Name="butSave" HorizontalAlignment="Right" Width="75" VerticalAlignment="Bottom" Height="24" Margin="0,0,10,9" Text="_Save" Click="butSave_Click" />
		<ui:TextBox x:Name="textName" Margin="113,18,0,0" Width="187" TabIndexOD="1" MaxLength="255"/>
		<ui:Label Margin="29,19,0,0" Text="Name" Width="84" HAlign="Right"/>
		<ui:Label Margin="29,41,0,0" Text="Abbreviation" Width="84" HAlign="Right"/>
		<ui:TextBox x:Name="textAbbreviation" Margin="113,40,0,0" Width="50" MaxLength="255"/>
		<ui:CheckBox x:Name="checkHidden" Margin="28,62,0,0" Checked="False" CheckAlign="MiddleRight" Text="Hidden" Width="98" />
	</Grid>
</local:FrmODBase>